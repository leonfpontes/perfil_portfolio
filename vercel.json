{
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "css/(.*)", "use": "@vercel/static" },
    { "src": "js/(.*)", "use": "@vercel/static" },
    { "src": "img/(.*)", "use": "@vercel/static" },
    { "src": "public/(.*)", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "index.html" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
